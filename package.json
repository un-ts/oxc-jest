{
  "name": "oxc-jest",
  "version": "0.0.0",
  "type": "module",
  "description": "A simple library boilerplate.",
  "repository": "git+https://github.com/un-ts/oxc-jest.git",
  "author": "JounQin <admin@1stg.me> (https://www.1stG.me)",
  "funding": "https://opencollective.com/unts",
  "license": "MIT",
  "packageManager": "yarn@4.9.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": "./src/index.ts",
  "files": [
    "lib",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "run-p 'build:*'",
    "build:tsc": "tsc -b src",
    "build:tsdown": "tsdown src/index.ts -d lib --dts --format cjs",
    "clean": "premove .type-coverage coverage dist lib '.*cache'",
    "dev": "vitest",
    "docs": "vite",
    "docs:build": "vite build",
    "format": "prettier --write .",
    "lint": "run-p 'lint:*'",
    "lint:es": "eslint . --cache --max-warnings 10",
    "lint:style": "stylelint . --cache",
    "lint:tsc": "tsc -b --noEmit",
    "prepare": "simple-git-hooks && yarn-berry-deduplicate || exit 0",
    "release": "yarn build && clean-pkg-json && changeset publish",
    "serve": "vite preview",
    "test": "vitest run",
    "typecov": "type-coverage",
    "version": "changeset version && yarn --no-immutable"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@1stg/browserslist-config": "^2.1.4",
    "@1stg/common-config": "^13.0.1",
    "@1stg/postcss-config": "^6.0.6",
    "@1stg/stylelint-config": "^6.0.5",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.4",
    "@commitlint/cli": "^19.8.1",
    "@mdx-js/rollup": "^3.1.0",
    "@shikijs/rehype": "^3.4.2",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/web": "^0.0.235",
    "@vercel/analytics": "^1.5.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/coverage-v8": "^3.1.3",
    "clean-pkg-json": "^1.3.0",
    "eslint": "^9.27.0",
    "github-markdown-css": "^5.8.1",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.2",
    "premove": "^4.0.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "simple-git-hooks": "^2.13.0",
    "size-limit": "^11.2.0",
    "stylelint": "^16.19.1",
    "tsdown": "^0.11.11",
    "type-coverage": "^2.29.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "yarn-berry-deduplicate": "^6.1.3"
  },
  "resolutions": {
    "prettier": "^3.5.3"
  },
  "publishConfig": {
    "main": "./lib/index.cjs",
    "types": "./lib/index.d.cts",
    "module": "./lib/index.js",
    "exports": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "typeCoverage": {
    "atLeast": 100,
    "cache": true,
    "detail": true,
    "ignoreAsAssertion": true,
    "ignoreFiles": [
      "**/*.d.ts"
    ],
    "ignoreNonNullAssertion": true,
    "showRelativePath": true,
    "strict": true,
    "update": true
  }
}
